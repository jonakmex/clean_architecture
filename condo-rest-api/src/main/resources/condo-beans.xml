<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="helloWorldInteraction" class="com.jonak.condo.admin.interaction.HelloWorldInteraction"/>

    <bean id="requestFactory" class="com.jonak.condo.admin.factory.RequestFactoryImpl">
        <constructor-arg name="basePackage" value="com.jonak.condo.admin.boundary"/>
    </bean>
</beans>